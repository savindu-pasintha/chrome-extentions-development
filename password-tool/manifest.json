{

    "manifest_version": 3,
    "name": "Password Generate Toolkit",
    "version": "0.0.1",
    "description" :"Include Features : 6-25 Length , Lowercase , Uppercase, Simbolic, Dublicates, Copy Paste , Editable & Customizable Support.",
    "action" : {
      "default_popup":"Password.html",
      "default_title":"Password Generate Toolkit",
      "default_icon":{
        "16":"./images/password_1_16x16.png",
        "48":"./images/password_2_48x48.png",
        "128":"./images/password_3_128x128.png" }
    },
    "icons":{
        "16":"./images/password_1_16x16.png",
        "48":"./images/password_2_48x48.png",
        "128":"./images/password_3_128x128.png"
    },
    
    "content_scripts":[{
        "matches":["<all_urls>"],
        "js":["main.js"],
        "css":["main.css"]
    }]
  
}   
    
    
   
